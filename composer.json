{
    "name": "jlis/php-prometheus",
    "description": "A PHP client for Prometheus.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Julius Ehrlich",
            "email": "julius@ehrlich-bros.de"
        },
        {
            "name": "Julius Ehrlich",
            "email": "julius@billomat.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jlis\\PhpPrometheus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jlis\\PhpPrometheus\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/jlis/prometheus_client_php.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "jimdo/prometheus_client_php": "dev-apcu"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7",
        "squizlabs/php_codesniffer": "^3.0@dev"
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    }
}
